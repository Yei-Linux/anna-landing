generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

model users {
  id               String            @id @default(auto()) @map("_id") @db.ObjectId
  phone            String
  fullName         String
  documentNumber   String?
  genderId         String?
  lastInteraction  Int?
  Diagnosis        diagnosis[]
  clinic_histories clinic_histories?
}

model diagnosis {
  id               String  @id @default(auto()) @map("_id") @db.ObjectId
  user             users   @relation(fields: [userId], references: [id])
  userId           String  @db.ObjectId
  title            String
  description      String?
  creationTime     String
  diagnosisDetails Json
}

model clinic_histories {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  user        users  @relation(fields: [userId], references: [id])
  userId      String @unique @db.ObjectId
  civilStatus String
  occupation  String
  background  Json
}

model doctors {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId
  email          String  @unique
  password       String
  fullName       String
  documentNumber String?
}
